	gpu: gpu@0x01800000 {
		device_type = "gpu";
		compatible = "img,gpu";
		reg = <0x0 0x01800000 0x0 0x80000>;
		interrupts = <GIC_SPI 97 IRQ_TYPE_LEVEL_HIGH>;
		interrupt-names = "IRQGPU";
		clocks = <&clk_pll_gpu>, <&clk_gpu>;
		clock-names = "clk_parent", "clk_mali";
		power-domains = <&pd_gpu>;
	};
