&lcd_fb0 {
	lcd_used = <1>;
	lcd_driver_name = "st7789v";
	lcd_x = <240>;
	lcd_y = <240>;
	lcd_width = <32>;
	lcd_height = <32>;
	lcd_data_speed = <60>;

	lcd_if = <0>;

	lcd_dbi_if = <4>;
	lcd_spi_bus_num = <0>;
	lcd_pixel_fmt = <0>;
	lcd_dbi_fmt = <3>;
	lcd_rgb_order = <0>;

	lcd_pwm_used = <0>;
	lcd_pwm_ch = <6>;
	lcd_pwm_freq = <5000>;
