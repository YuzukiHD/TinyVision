menu "MBUS Drivers"
	depends on AW_BSP

config AW_MBUS_SUN50IW9
	tristate "Mbus Support for Allwinner SoCs SUN50IW9"
	depends on ARCH_SUN50IW9
	select HWMON
	help
	  Driver supporting the mbus for sunxi platforms of sun50iw9.

